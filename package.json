{
  "name": "galnetpedia",
  "version": "1.0.0",
  "description": "Galnet",
  "main": "index.js",
  "repository": "git@github.com:pareco-spike-team/research.git",
  "author": "Cmdr Clannad <clannad@spam.org>",
  "license": "MIT",
  "scripts": {
    "preinstall_packages": "npm install -g yarn@1.15.2",
    "install_packages": "yarn install",
    "precopy": "yarn run install_packages && if [ -d './dist' ]; then rm -Rf ./dist; fi && mkdir -p dist/fontawesome",
    "copy": "cp -a ./node_modules/@fortawesome/fontawesome-free/. ./dist/fontawesome/",
    "prebuild": "yarn run copy && cp ./client/src/index.html ./dist/index.html",
    "build": "./node_modules/.bin/elm make ./client/src/Main.elm --output=./dist/galnetpedia.js --debug",
    "build_release": "yarn prebuild && ./node_modules/.bin/elm make ./client/src/Main.elm --output=./dist/galnetpedia.js"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "~5.8.1",
    "an-array-of-english-words": "~1.3.1",
    "body-parser": "~1.18.3",
    "compression": "~1.7.4",
    "cookie-parser": "~1.4.4",
    "express": "~4.16.4",
    "express-session": "~1.16.1",
    "helmet": "~3.16.0",
    "moment": "~2.24.0",
    "morgan": "~1.9.1",
    "neo4j-driver": "~1.7.3",
    "p3p": "~0.0.2",
    "request-promise-native": "~1.0.7",
    "shortid": "~2.2.14"
  },
  "devDependencies": {
    "elm": "~0.19.0-bugfix6",
    "elm-format": "~0.8.1",
    "jshint": "~2.10.2"
  }
}
